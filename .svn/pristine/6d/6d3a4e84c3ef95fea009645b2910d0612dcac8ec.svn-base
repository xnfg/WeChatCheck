package com.school.wechat.wechatserver.util;

public class Area {

    public static String getArea(Integer areaNumber){
        areaNumber+=1;
        String str;
        switch (areaNumber){
            case 0:
                str="天心区";
            case 1:
                str="岳麓区";
            case 2:
                str="开福区";
            case 3:
                str="雨花区";
            case 4:
                str="长沙县";
            case 5:
                str="望城区";
            case 6:
                str="宁乡县";
            case 7:
                str="浏阳市";
            case 8:
                str="芙蓉区";
            default:
                str=null;
        }
        return str;
    }
}
